<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="StackOverflow Automation Test Suite">
	<listeners>
		<listener class-name="com.stackOverflow.qa.ExtentReport.ExtentReportListener" />
	</listeners>
	<test name="Regression Suite">
		<classes>
			<class name="com.stackOverflow.qa.testcases.StackOverFlowPageTest" />
			<class name="com.stackOverflow.qa.testcases.QuestionsPageTest" />
			<class name="com.stackOverflow.qa.testcases.TagsPageTest" />
		</classes>
	</test>
</suite>